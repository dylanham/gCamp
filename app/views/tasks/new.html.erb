<div class ="container">
  <div class ="page-header">
  <h1>New Task </h1>
  </div>
  <%= form_for(@task, html: {class: 'form-horizontal'}) do |f| %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.submit "Create Task" %>
  <% end %>
  <%= link_to 'Cancel', tasks_path, class: 'btn btn-default' %>
</div>
